{
    "compilerOptions": {
        "target": "ESNext",
        "useDefineForClassFields": true,
        "module": "ESNext",
        "moduleResolution": "Node",
        "strict": true,
        "jsx": "preserve",
        "sourceMap": true,
        "resolveJsonModule": true,
        "isolatedModules": true,
        "esModuleInterop": true,
        "lib": ["ESNext", "DOM"],
        "skipLibCheck": true,
        "outDir": "dist",
        "declaration": true,
        "typeRoots": [
            "types"
        ],
    },
    "files": [],
    "include": [
        "lib/**/*.ts",
        "lib/**/*.d.ts",
        "lib/**/*.tsx",
        "lib/**/*.vue",
        "node_modules/@types/**/*.ts"
, "lib/composables/useTanstackQueryHelpers.test.ts_old", "lib/tests/refreshPartialItemInTanstackCache.spec.ts_old"    ],
    "references": [
        { "path": "./tsconfig.app.json" },
        { "path": "./tsconfig.node.json" }
    ]
}
